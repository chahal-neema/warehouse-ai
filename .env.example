# MCP Configuration
MCP_SERVER_HOST="localhost"
MCP_SERVER_PORT="8081"

# Agent Configuration
ORCHESTRATOR_PORT="3000"
LOCATION_AGENT_PORT="3001"

# Logging
LOG_LEVEL="info"
LOG_FORMAT="json"

# Environment
NODE_ENV="development"